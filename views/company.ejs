<!DOCTYPE html>
<html>
<head>
	<title>工作沙盤</title>
	<!-- Bootstrap 5.2.0 CSS only -->
	<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-gH2yIJqKdNHPEq0n4Mqa/HGKIhSkIHeL5AyhkYV8i59U5AR6csBvApHHNl/vI1Bx" crossorigin="anonymous">
	<!-- vue -->
	<script src="https://unpkg.com/vue@3"></script>
</head>


<body>
	<div class="container mt-4" id="app">
		<table class="table table-bordered">
			<thead>
				<tr>
					<th scope="col">等泥作</th>
					<th scope="col">油漆完成</th>
					<th scope="col">清潔完成</th>
					<th scope="col">門牌號碼</th>
					<th scope="col">戶號</th>
					<th scope="col">目標日期</th>
					<th scope="col">已排驗屋</th>
					<th scope="col">給業務約</th>
					<th scope="col">優先戶</th>
					<th scope="col">已複驗</th>
					<th scope="col">可交屋</th>
        			</tr>
			</thead>
			<tbody>
				<tr v-for="item in haha">
					<td>{{ item.等泥作 }}</td>
					<td>{{ item.油漆完成 }}</td>
					<td>{{ item.清潔完成 }}</td>
					<td>{{ item.門牌號碼 }}</td>
					<td>{{ item.戶號 }}</td>
					<td>{{ item.目標日期 }}</td>
					<td>{{ item.已排驗屋 }}</td>
					<td>{{ item.給業務約 }}</td>
					<td>{{ item.優先戶 }}</td>
					<td>{{ item.已複驗 }}</td>
					<td>{{ item.可交屋 }}</td>
				</tr>
			</tbody>
		</table>
	</div>

	<script>
		const { createApp } = Vue
		createApp({
			data() {
				return {
					message: 'Hello Vue!',
					haha: <%- test %>
				}
    			}
 		}).mount('#app')
	</script>

</table>
</body>
</html>
